{
    "IPRestriction": {
        "EnableIPRestriction": true, // IP 제한 기능 활성화
        "CollectLoginIP": true // 로그인 시 IP 수집 활성화
    },
    "TenantName": "VisualAcademy",
    "ConnectionStrings": {
        "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=VisualAcademy;Trusted_Connection=True;MultipleActiveResultSets=true"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*"
}
